{
  "name": "mad-lab-workbench",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:3001": "next dev -p 3001",
    "dev:test": "next dev -p 3010",
    "build": "pnpm -w build:webview",
    "start": "next start",
    "start:3001": "next start -p 3001",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run tests/unit/",
    "test:integration": "vitest run tests/integration/",
    "test:api": "vitest run tests/api/",
    "test:components": "vitest run tests/ui/ tests/widgets/",
    "test:security": "vitest run tests/security/",
    "test:performance": "vitest run tests/performance/",
    "test:accessibility": "vitest run tests/accessibility/",
    "test:visual": "vitest run tests/visual-regression/",
    "test:contracts": "vitest run tests/contracts/",
    "test:contracts:consumer": "vitest run tests/contracts/ --grep='Contract Tests' --reporter=verbose",
    "test:contracts:provider": "vitest run tests/contracts/provider-verification.test.ts --reporter=verbose",
    "test:contracts:publish": "pact-broker publish pacts --consumer-app-version=$npm_package_version",
    "test:coverage": "vitest run --coverage",
    "test:report": "vitest run --reporter=json --outputFile=test-results/results.json",
    "test:ci": "npm run lint && npm run typecheck && npm run test:coverage",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:api && npm run test:security && npm run test:performance && npm run test:accessibility && npm run test:contracts",
    "e2e": "playwright test",
    "dev:web": "next dev",
    "build:web": "next build",
    "build:webview": "next build && rm -rf apps/extension/dist/webview && mkdir -p apps/extension/dist/webview && cp -R .next/static .next/server apps/extension/dist/webview/",
    "dev:ext": "pnpm --filter madlab-workbench dev"
  },
